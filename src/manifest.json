{
  "name": "Page load time",
  "version": "3.2.2",
  "manifest_version": 3,
  "description": "Displays page load time in the toolbar",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "stopwatch16.png",
      "48": "stopwatch48.png",
      "128": "stopwatch128.png"
    }
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["performance.js"]
  }],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "stopwatch16.png",
    "48": "stopwatch48.png",
    "128": "stopwatch128.png"
  }
}